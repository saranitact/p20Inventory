properties([pipelineTriggers([githubPush()])])
pipeline {
    agent { label 'master' }

    stages {
        stage('Hello with Github webhook test') {
            steps {
                echo 'Hello World updated'
            }
        }
        stage("Git CheckoutInventory"){
           steps{
                git branch: 'master', url: 'https://github.com/saranitact/Inventory'
                  echo 'Hello from Inventory checkout step'
            }
        }
        stage('gitcheckout from code snippet'){
            steps{
                checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], 
                submoduleCfg: [], userRemoteConfigs: [[credentialsId: '8a226c3a-9b44-4174-b38b-dff5aa1cd0d4', url: 'https://github.com/saranitact/p20Inventory/']]])
            }
        }
    }
    
}

