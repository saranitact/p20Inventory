pipeline {
    agent { label 'master' }

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage("Git Checkout"){
           steps{
                git credentialsId: 'github', url: 'https://github.com/saranitact/p20Inventory'
            }
        }
        stage('gitcheckout from code snippet to test Webhook via plugin'){
            steps{
                echo 'Hello World checkout from VS Code'
                checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], 
                submoduleCfg: [], userRemoteConfigs: [[credentialsId: '8a226c3a-9b44-4174-b38b-dff5aa1cd0d4', url: 'https://github.com/saranitact/p20Inventory/']]])
            }
        }
    }
    
}

